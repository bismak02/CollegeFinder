}


void intro() {
	cout << "Hello! Welcome to the University Program!\n";
	cout << "Which state would you like to attend University in? " << endl;

	cout << left << setw(25) << "Alabama = 0 " << left << setw(25) << "Alaska = 1 " << left << setw(25) << "Arizona = 2 " << left << setw(25) << "Arkansas = 3 " << left << setw(25) << "California = 4 " << endl;
