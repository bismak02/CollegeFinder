void choose_important() {
	cout << endl;
	cout << "What do you prioritize most in a University?" << endl;
	cout << left << setw(20) << "Campus = 0" << left << setw(20) << "Class Size = 1" << left << setw(20) << "Professors = 2" << endl << left << setw(20) << "Cheap Tuition = 3" << left << setw(20) << "Campus Life = 4" << left << setw(20) << " Diversity = 5" << endl;
	cout << "Please Enter a Number: ";
}

void choose_major() {
	cout << endl;
	cout << "What would you like to major in?" << endl;
	cout << left << setw(30) << "Agriculture = 0" << left << setw(30) << "Architecure = 1" << left << setw(30) << "Arts = 2" << endl;
	cout << left << setw(30) << "Business = 3" << left << setw(30) << "Communications = 4" << left << setw(30) << "Computer Science = 5" << endl;
	cout << left << setw(30) << "Education = 6" << left << setw(30) << "Engineering = 7" << left << setw(30) << "Religion = 8" << endl;
	cout << left << setw(30) << "Science = 9" << left << setw(30) << "Social Science = 10" << endl;
	cout << "Please Enter a Number: ";
}
